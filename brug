# Gruba Katıldı Mesajı
@Murti.on(events.ChatAction)
async def handler(event):
    if event.user_joined:
        await event.reply(f"{random.choice(yeni_kisi)}, {random.choice(hgemoji)}")

murti_start = b"t.me/uslanmazmurti"

@Murti.on(events.NewMessage(pattern='(?i)/start+'))
async def yeni_mesaj(event: events.NewMessage.Event):
    msg = await event.reply("🔄 **Başlatılıyor..**")
    sleep(1)
    await msg.delete()
    s = await event.reply("⚡")
    sleep(0.6)
    c = await s.edit("🎉")
    sleep(1)
    m = await c.edit("✅ **Başarıyla Başlatıldı..**")
    sleep(1)
    await m.edit("""
     🤖 **ᴍᴇʀʜᴀʙᴀ, ʙᴇɴ ʙᴏʀᴀ sᴏʜʙᴇᴛ ʙᴏᴛ

🌠 sᴏʜʙᴇᴛ ᴠᴇ ᴏʏᴜɴ ʙᴏᴛᴜʏᴜᴍ

💘 ᴍᴀʀɪғᴇᴛʟᴇʀɪᴍɪ ɢöʀᴍᴇᴋ ɪçɪɴ 
/yardim ᴋᴏᴍᴜᴛᴜɴᴜ ᴋᴜʟʟᴀɴıɴ! 

💥 sᴛᴀʙɪʟ çᴀʟışᴍᴀᴍ ɪçɪɴ, 
ʙᴇɴɪ ɢʀᴜʙᴜɴᴜᴢᴀ ᴇᴋʟᴇʏᴇʀᴇᴋ,

📌 s‌ᴜ ʏᴇᴛᴋɪʟᴇʀɪ ᴠᴇʀɪɴ﹕

﹡ ᴍᴇsᴀᴊʟᴀʀı sɪʟᴍᴇ**
"""    
)
    
@Murti.on(events.NewMessage(pattern='(?i)/yardim+'))
async def yeni_mesaj(event: events.NewMessage.Event):
    await event.reply(f"""
    ✹> __@BoraSohbetBot Hakkında__

**➜ Selam Alır Veririm, 
➜ Nasıl Olduğunu Sorarım, 
➜ Gruba Yeni Katılanları Karşılarım, 
➜ Zar Ve Dart Atarım, 
➜ Doğruluk Ve Cesaret Soruları Sorarım,
➜ Ansızın Sohbetinize Karışırım, 
➜ Küfür Edenleri Nazikce Uyarırım,
➜ Sabah Olduğunda Günaydın Mesajı Gönderirim, 
➜ Gece Olduğunda İyi Geceler Mesajı Gönderirim,
➜ Biri Trip Atığında Kızarım,
➜ Adminlerden Biri Ban Veya Mute Komutlarını Kullandığı Zaman Överim Yada Gömerim**,

● **Oyun Listesine Erişmek İçin** `Oyna` **Yaz.

● Kısacası Yalnız Hissediyorsan Ben Sana İlaç Gibi Geleceğim.** {random.choice(hgemoji)}
"""
)

